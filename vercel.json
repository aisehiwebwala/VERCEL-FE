{
    "version": 2,
    "builds": [
      {
        "src": "./SERVER/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "./CLIENT/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "./SERVER/api.js"
      },
      {
        "src": "/(.*)",
        "dest": "./CLIENT/dist/index.html"
      }
    ]
  }